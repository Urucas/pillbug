    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.13.1",
    "minimist": "^1.1.1",
    "node-emoji": "^1.0.4",
    "openurl": "^1.1.0",
    "request": "^2.60.0"
